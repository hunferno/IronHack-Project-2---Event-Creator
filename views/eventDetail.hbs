<header class="imageAll">
    <div class="header-event-details">
        <img src="{{image}}" alt="">
        <h1>{{title}}</h1>
        <p>{{location.city}}</p>
    </div>
</header>

<main id="main-eventDetail">
    <section id="image-event">
        {{!-- <img src="{{image}}" alt=""> --}}
    </section>
    <section id="section1">
        <div>
        </div>

        <div id="informationEvent">
            <div class="test">
                <img src="https://res.cloudinary.com/dqwbskuge/image/upload/v1608153997/calendar_yneyxb.png" />
                <p><strong>Start Date</strong> : {{dateStart}}</p>
                <p><strong>End Date</strong> : {{dateEnd}}</p>
            </div>
            <div class="test">
                <img src="https://res.cloudinary.com/dqwbskuge/image/upload/v1608153997/clock_tr3x3q.png">
                <p><strong>Start Hour</strong> : {{startTime}}</p>
                <p><strong>End Hour</strong> : {{endTime}}</p>
            </div>
            <div class="test">
                <img src="https://res.cloudinary.com/dqwbskuge/image/upload/v1608153997/pin_hbwgr0.png">
                <p><strong>address</strong> : <br>
                    {{location.adress}} <br>
                    {{location.zipCode}} - {{location.city}} </p>
            </div>
        </div>
    </section>

    <section id="descriptionEvent">
        <div id="descriptionDetails">
            <p class="des-1">Details</p>
            <p class="des-1">Created by : {{id_user.name}}</p>
            <p class="des-2">{{description}}</p>
        </div>

        <div id="container-button">
            {{#if full}}
            <p>Full</p>
            {{/if}}

            {{#if isLoggedIn}}
            {{#if participate}}
            <div class="button-participate-event">
                <a class="button-participate" href="/event/{{_id}}/quit">Quit Event</a>
            </div>
            {{else}}
            <div class="button-participate-event">
                <a class="button-participate" href="/event/{{_id}}/participate">Participate</a>
            </div>
            {{/if}}
            {{/if}}
            {{#unless isLoggedIn}}
            <div class="text-participate">
                <p>If you want to participate <br> Please SIGN</p>
            </div>
            <div class="button-sign-signout">
                <a class="sign-event" href="/signin">Signin</a>
                <a class="sign-event" href="/signup">Signup</a>
            </div>
            <div class="localisation">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2624.142047744348!2d2.3354330160472316!3d48.87456857928921!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e66e38f817b573%3A0x48d69c30470e7aeb!2sIronhack!5e0!3m2!1sen!2ses!4v1495208746099"
                    class="map-frame" frameborder="0" style="border:0" allowfullscreen></iframe>
            </div>
            {{/unless}}
        </div>

        {{!-- <div id="container-button">
            {{#if full}}
