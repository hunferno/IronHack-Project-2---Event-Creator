<section class="py-5">
    <div class="container-page-event">
        <h1 class="h2 mb-5">Update your event </h1>
        <form id="form-page-event" action="/events_manage/update/{{_id}}" method="post" enctype="multipart/form-data">
            {{!-- enctype="multipart/form-data" --}}
            {{!-- <h4>Event name</h4> --}}
            <p class="text-muted text-sm">Indicate the title of your event</p>
            <div class="">
                <div class="">
                    <label class="form-label" for="form_name">Name of event</label>
                    <input class="form-control" name="title" id="form_name" value="{{title}}">
                </div>

                {{!-- <div class="form-group">
                    <label class="form-label" for="form_type">Type of event</label>
                    <select class="selectpicker form-control" name="eventType" id="form_type"
                        data-style="btn-selectpicker" title="" aria-describedby="propertyTypeHelp">
                        {{#if public}}
                        <option value="public" selected="selected">Public</option>
                        <option value="private" selected="">Private</option>
                        {{else}}
                        <option value="public" selected="">Public</option>
                        <option value="private" selected="selected">Private</option>
                        {{/if}}
                    </select>
                </div> --}}
            </div>

            <div class="row form-block">
                <div class="col-lg-4">
                    <h4>Description</h4>
                    <p class="text-muted text-sm"> Describe your event to us so that we can put it online. This step is
                        important
                        to provide as much information as possible about your event..</p>
                </div>
                <div class="col-lg-7 ml-auto">
                    <div class="form-group mb-5">
                        <textarea class="form-control" name="description" id="form_description" rows="5"
                            aria-describedby="descriptionHelp">{{description}}</textarea>
                    </div>
                </div>
            </div>

            <div id="form-date">
                <div id="start-date">
                    <label class="label-date" for="start">Start date:</label>
                    <input class="input-date" type="date" id="start" name="dateStart" value="{{dateStart}}"
                        min="2018-01-01" max="2021-12-31">
                </div>

                <div id="end-date">
                    <label class="label-date" for="start">End date:</label>
                    <input class="input-date" type="date" id="end" name="dateEnd" value="{{dateEnd}}" min="2018-01-01"
                        max="2021-12-31">
                </div>
            </div>

            <div id="time-form">
                <div>
                    <label for="appt">start time meeting:</label>
                    <input type="time" id="appt" name="startTime" min="09:00" max="18:00" value="{{startTime}}"
                        required>
                </div>
                <div>
                    <label for="appt">end time meeting:</label>
                    <input type="time" id="appt" name="endTime" min="09:00" max="18:00" value="{{endTime}}" required>
                </div>
            </div>

            <div class="row form-block">
                <div class="col-lg-4">
                    <h4>Participants</h4>
                    <p class="text-muted text-sm">Indicate the number of places available in your event</p>
                </div>
                <div class="col-lg-7 ml-auto">
                    <div class="">
                        <div class="">
                        </div>
                        <label for="tentacles">Number of participants:</label>
                        <input type="number" id="tentacles" name="maxParticipant" min="0" max="100000"
                            value="{{maxParticipant}}">
                    </div>
                </div>

                <div class="">
                    <div class="">
                        <h4>Photo</h4>
                        <p class="text-muted text-sm">Add a photo of your event to attract as many participants as
                            possible</p>
                    </div>
                    <div class="box__input">
                        <input class="box__file" type="file" name="image" id="file"
                            data-multiple-caption="{count} files selected" multiple value="{{image}}" />
                    </div>

                </div>
                <div class="row form-block">
                    <div class="col-lg-4">
                        <h4>Address</h4>
